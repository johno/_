{
  "private": true,
  "name": "johno.com",
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "deploy": "now switch johno && now && now alias $(pbpaste) johno.com && now alias $(pbpaste) www.johno.com",
    "now-build": "yarn build"
  },
  "dependencies": {
    "gatsby": "2.18.1",
    "gatsby-plugin-fathom": "1.1.0",
    "gatsby-theme-austere": "0.0.11",
    "gatsby-theme-blog-tags": "0.0.11",
    "husky": "3.1.0",
    "lint-staged": "9.4.3",
    "prettier": "1.19.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "three": "0.109.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md,mdx,json}": [
      "prettier --write --no-semi --single-quote",
      "git add"
    ]
  },
  "resolutions": {
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "@babel/helpers": "^7.6.0"
  }
}
